    "version": "0.2.0",
    "configurations": [

        {
            "name": "Debug (Skia)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart", 
            "args": ["--no-enable-impeller"], 
                   
        },
        {
            "name": "Debug (Impeller)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart", 
            "args": ["--enable-impeller"], 
                   
        },        
        {
            "name": "Profile (Skia)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main.dart",
            "args": ["--no-enable-impeller"], 
        
        },
        {
            "name": "Profile (Impeller)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main.dart",
            "args": ["--enable-impeller"], 
                   
        },
    ]
}